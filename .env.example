# Instagram Graph API Configuration
INSTAGRAM_APP_ID=your_app_id_here
INSTAGRAM_APP_SECRET=your_app_secret_here
INSTAGRAM_REDIRECT_URI=http://localhost:4321/instagram-callback

# Instagram Long-lived Token (generated via OAuth flow)
# This token gets refreshed automatically by GitHub Actions every 30 days
INSTAGRAM_LONG_LIVED_TOKEN=your_long_lived_token_here
INSTAGRAM_TOKEN_EXPIRES_AT=timestamp_when_token_expires

# Storyblok Configuration
STORYBLOK_TOKEN=your_storyblok_token_here_url

# Note: You can remove the old token once OAuth is set up
# INSTAGRAM_ACCESS_KEY=your_old_token_here
